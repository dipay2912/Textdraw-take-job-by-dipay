PlayerTextDrawUseBox(playerid, DipayJob[playerid][13], 0);
PlayerTextDrawSetProportional(playerid, DipayJob[playerid][13], 1);
PlayerTextDrawSetSelectable(playerid, DipayJob[playerid][13], 0);


